{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "aws/aws-sdk-php": "^3.202",
        "aws/aws-sdk-php-laravel": "^3.7",
        "barryvdh/laravel-translation-manager": "^0.5.10",
        "doctrine/dbal": "^3.6",
        "fruitcake/laravel-cors": "^2.0",
        "ghanem/laravel-smsmisr": "^3.0",
        "guzzlehttp/guzzle": "^7.5",
        "intervention/image": "^2.7",
        "jantinnerezo/livewire-alert": "^2.2",
        "laravel/breeze": "^1.4",
        "laravel/framework": "^8.65",
        "laravel/sanctum": "^2.11",
        "laravel/socialite": "^5.6",
        "laravel/telescope": "^4.14",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-sftp": "~1.0",
        "livewire/livewire": "^2.7",
        "maatwebsite/excel": "^3.1",
        "mcamara/laravel-localization": "^1.6",
        "nexmo/client": "^2.4",
        "protoqol/prequel": "^1.23",
        "socialiteproviders/facebook": "^4.1",
        "socialiteproviders/google": "^4.1",
        "spatie/laravel-permission": "^5.3",
        "spatie/laravel-translatable": "^5.0",
        "staudenmeir/laravel-adjacency-list": "^1.0",
        "stechstudio/laravel-zipstream": "^4.13",
      "ext-json": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
